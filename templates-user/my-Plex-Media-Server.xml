  <Config Name="Host Path 3" Target="/data" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/Plex-Media-Server/data</Config>
  <Config Name="Key 1" Target="PLEX_CLAIM" Default="Insert Token from https://plex.tv/claim" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">Insert Token from https://plex.tv/claim</Config>
